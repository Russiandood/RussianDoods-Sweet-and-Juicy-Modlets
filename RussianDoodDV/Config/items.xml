<?xml version="1.0" encoding="utf-8"?>

<configs>
	<append xpath="/items">
		<item name="Duke's Voucher">
			<property name="DescriptionKey" value="dukeVoucherDesc"/>
			<property name="CustomIcon" value="dukeVoucher" />
			<property name="HoldType" value="45"/>
			<property name="Meshfile" value="Items/Misc/sackPrefab"/>
			<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab"/>
			<property name="Material" value="Morganic"/>
			<property name="Stacknumber" value="30000"/> <!-- STK resource -->
			<property name="EconomicValue" value="5000"/>
			<property name="EconomicBundleSize" value="1"/>
			<property name="SellableToTrader" value="false"/>
			<property name="CraftingIngredientExp" value="0"/>
			<property name="CraftingIngredientTime" value="0.01"/>
		</item>
	</append>
	
	<append xpath="/items/item[@name='casinoCoin']">
		<property name="CraftingIngredientTime" value="0.000001"/>
		<property name="CraftingIngredientExp" value="0"/>
	</append>
